        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:ManagementProject.UserControls"
        mc:Ignorable="d"
        Title="TrackManageButton" Height="50" Width="50" ShowInTaskbar="False"  ResizeMode="NoResize" Opacity="0.9"  OpacityMask="White"  AllowsTransparency="True" Background="Transparent" WindowStyle="None">
    <Grid>
        <Button 
                Style="{StaticResource GelButton}"  
                Command="{Binding OpenTrackerCommand}"
                 Width="50" Height="50">
