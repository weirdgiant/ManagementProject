<Window x:Class="ManagementProject.UserControls.TrackPageControls.TrackCameraWin"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:UserControls="clr-namespace:ManagementProject.UserControls"
        mc:Ignorable="d"
        Title="TrackCameraWin" Height="1030"
        Width="323"
        ShowInTaskbar="False" 
        ResizeMode="NoResize" 
        Opacity="0.95" 
        AllowsTransparency="True"
        Background="Transparent" 
        BorderThickness="0 0 0 0"
        WindowStyle="None">

    <Window.Effect>
        <DropShadowEffect BlurRadius="24" Color="Black"/>
    </Window.Effect>

    <Grid>
        <UserControls:TrackCameraList x:Name="tracker"  Margin="0,0,0,0" />
    </Grid>
</Window>
