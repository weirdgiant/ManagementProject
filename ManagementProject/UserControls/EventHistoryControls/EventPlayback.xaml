<UserControl  x:Class="ManagementProject.UserControls.EventHistoryControls.EventPlayback"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:ManagementProject.UserControls.EventHistoryControls"
        mc:Ignorable="d"
        xmlns:i="clr-namespace:System.Windows.Interactivity;assembly=System.Windows.Interactivity"
        xmlns:forms="clr-namespace:System.Windows.Forms;assembly=System.Windows.Forms"
        d:DesignHeight="800" d:DesignWidth="1000" >

    <Grid Name="grid" Background="Gray">
        <WindowsFormsHost Grid.Row="0" Margin="5" Background="Gray">
            <forms:PictureBox x:Name="playWindow"></forms:PictureBox>
        </WindowsFormsHost>
    </Grid>
</UserControl>
