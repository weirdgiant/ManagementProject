<Page x:Class="ManagementProject.PageView.AlarmPage"
      xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
      xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
      xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
      xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
      xmlns:local="clr-namespace:ManagementProject.PageView"
      mc:Ignorable="d" 
      d:DesignHeight="450" d:DesignWidth="800"
      Title="AlarmPage">

    <Grid Background="#FF252525">
        <Grid.ColumnDefinitions>
            <ColumnDefinition Width="2*"/>
            <ColumnDefinition Width="1*"/>
            <ColumnDefinition Width="30"/>
        </Grid.ColumnDefinitions>
        <Grid Grid.Column="0"/>
        <Grid Grid.Column="1"/>
        <Grid  Grid.Column="2">
            <Button x:Name="button" Style="{StaticResource {x:Static ToolBar.ButtonStyleKey} }" Margin="0,0,0,0" Height="40" VerticalAlignment="Bottom" BorderBrush="{x:Null}" HorizontalAlignment="Stretch">
                <TextBlock Text="返回" TextWrapping="Wrap" Width="15" Foreground="White"/>
            </Button>
        </Grid>

    </Grid>
</Page>
